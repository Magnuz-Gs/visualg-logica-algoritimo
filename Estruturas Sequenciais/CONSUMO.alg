Algoritmo "consumo"

Var
    distancia : inteiro
    combustivel, consumo :real
    

Inicio
       escreva("Distancia percorrida: ")
       leia(distancia)
       escreva("combustivel gasto: ")
       leia(combustivel)
       
       consumo <- distancia / combustivel
       
       escreva("cnsumo medio = ", consumo:6:3)

Fimalgoritmo